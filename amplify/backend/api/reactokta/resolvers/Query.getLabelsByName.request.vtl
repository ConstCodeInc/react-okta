{
    "version" : "2017-02-28",
    "operation" : "Scan",
    "limit": $util.defaultIfNull($ctx.args.limit, 1000),
  	"nextToken": $util.toJson($context.args.nextToken),
    "filter" : {
        "expression": "contains (:labels, label)",
        "expressionValues" : {
            ":labels" : $util.dynamodb.toDynamoDBJson($context.args.labelNames)
        }
    }
}