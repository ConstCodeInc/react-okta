{
  "version": "2017-02-28",
  "operation": "Scan",
  "limit": 100,
  "nextToken": $util.toJson($context.args.nextToken),
  "filter": {
    "expression": "#genreAttr IN :genreValues",
    "expressionNames": {
      "#genreAttr": "genre"
    },
    "expressionValues": {
      ":genreValues": {
        "SS": $util.toJson(["Pop", "Classical", "Rock"])
      }
    }
  }
}